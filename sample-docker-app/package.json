{
  "name": "sample-docker-app",
  "version": "1.0.0",
  "description": "A sample ActyxOS docker app that sends pings and displays them",
  "repository": {
    "type": "git",
    "url": "https://github.com/Actyx/quickstart"
  },
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node index",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:images": "npm run build && npm run build:image:x86_64 && npm run build:image:arm64v8",
    "build:image:x86_64": "docker build -t sample-docker-app-x86_64 .",
    "build:image:arm64v8": "docker build -f Dockerfile.arm64v8 -t sample-docker-app-arm64v8 ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actyx/pond": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^13.9.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
